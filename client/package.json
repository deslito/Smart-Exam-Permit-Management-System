{
  "name": "kyambogo-university-smart-exam-permit-management-system",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "setenv:dev": "cross-env ENV_FILE=.env.development node scripts/setenv.js",
    "setenv:prod": "cross-env ENV_FILE=.env.production node scripts/setenv.js",
    "start:dev": "npm run update-env-ip && npm run setenv:dev  && npx expo start -c",
    "start:prod": "npm run setenv:prod && expo start --no-dev --minify",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "update-env-ip": "node scripts/update-env-ip.js",
    "build:web:prod": "npm run setenv:prod && npx expo export --platform web --clear"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
