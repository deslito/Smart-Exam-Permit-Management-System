    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.0",
    "cross-env": "^7.0.3",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.5",
    "react-native-dotenv": "^3.4.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
